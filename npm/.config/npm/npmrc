# Run this to ignore future changes:
# git update-index --assume-unchanged ($env.XDG_CONFIG_HOME | path join dotfiles npm .config npm npmrc)
#
# To undo:
# git update-index --no-assume-unchanged ($env.XDG_CONFIG_HOME | path join dotfiles npm .config npm npmrc)

# Global
prefix=${XDG_DATA_HOME}/npm
cache=${XDG_CACHE_HOME}/npm
fund=false
init-module=${XDG_CONFIG_HOME}/npm/config/npm-init.js
